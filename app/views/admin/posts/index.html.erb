<h1>Admin::Posts#index</h1>
<p>Find me in app/views/admin/posts/index.html.erb</p>

<% @posts.each do |post| %>
  <div class="mt-">
    <%= link_to post_path(post.id) do %>
      <p><%= image_tag 'sample-author1.jpg' %></p>
    <% end %>
    <p>活動名：<%= post.title %><p>
    <p>活動内容：<%= post.body %></p>
  </div>

  <%= link_to "削除", admin_post_path(post), method: :delete %>
<% end %>

<%= link_to "back", admin_root_path %>